<h1>Boards#create</h1>
<p>Find me in app/views/boards/create.html.erb</p>
<%= form_for @board, :url => {action: "create"}, :html => { :multipart => true } do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %> <br>
  <%= f.label :description %>
  <%= f.text_field :description %> <br>
  <%= f.label :image %>
  <%= f.file_field :image %> <br>
  <%= f.submit "Create" %>
<% end %>

